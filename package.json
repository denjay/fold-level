{
	"name": "fold-level",
	"displayName": "fold-level",
	"description": "Add fold level button on the status bar. Change fold level quickly.",
	"publisher": "denjay",
	"icon": "assets/fold-level.png",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/denjay/fold-level.git"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"configuration": {
			"type": "object",
			"title": "fold-level",
			"properties": {
				"foldLevel.level": {
					"type": "array",
					"description": "Set the display of the fold level button on the status bar.",
					"items": {
						"type": "string",
						"enum": [
							"âˆž",
							"1",
							"2",
							"3",
							"4",
							"5",
							"6",
							"7"
						]
					},
					"uniqueItems": true
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}